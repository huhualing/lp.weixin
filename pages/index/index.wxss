.back{
  color: #666;
}
swiper{
 width: 100%;

}swiper-item{
  position: relative;
  width: 100%;
}
.index_image{

  width: 100%;
}
.index_title{
  width: 100%;
  padding: 10rpx 0 10rpx 50rpx;
  position: absolute;
  bottom: 60rpx;
  font-size: 25rpx;
  background-color: rgba(0,0,0,0.2);
  color:white;
  font-weight: 400;
}
.icon_list{
  width: 90%;
  padding:20rpx 5% 10rpx 5%;
  display: flex;
  justify-content: space-between;
  flex-wrap: wrap;
  border-bottom: 1rpx solid #eee;
}
.icon{
  margin: 10rpx 20rpx;
  font-size: 25rpx;
  text-align: center;
}
.icon image{
  width: 100rpx;
  height: 100rpx;
}
.new_title{
  width: 90%;
  padding: 20rpx 5%;
  font-size: 25rpx;
  border-bottom: 1rpx solid #eee;
}
.type-box{
  width: 90%;
  padding: 20rpx 5%;
  border-bottom: 1rpx solid #eee;
}
.type-box .type-title{
  font-size: 28rpx;
  color: black;
  font-weight: 500;
  margin-bottom: 10rpx;
  
}
.type-box image{
  width: 100%;
}
.type-box .type-text{
  font-size: 23rpx;
  width: 100%;
  height: 60rpx;
  overflow: hidden;
}
